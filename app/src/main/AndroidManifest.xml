<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myintent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/ThirdTheme">
        <activity android:name=".modul.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".modul.profile.ProfileActivity"></activity>
        <activity android:name=".modul.task.list.ListActivity"></activity>
        <activity android:name=".modul.task.input.InputActivity"></activity>
        <activity android:name=".modul.task.edit_delete.EditDeleteActivity"></activity>
    </application>

</manifest>